<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Question"
    android:background="@color/application_background"
    >

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_button" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:text="Host Android App"
        android:textColor="@color/text_color"
        android:fontFamily="monospace"
        android:textSize="24dp"
        app:layout_constraintBottom_toTopOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355" />

    <View
        android:id="@+id/view7"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/hover_color"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        tools:layout_editor_absoluteX="3dp"
        tools:ignore="MissingConstraints" />

    <ScrollView
        android:layout_marginTop="4dp"
        android:layout_width="match_parent"
        android:layout_height="848dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:layout_editor_absoluteX="1dp"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >


            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="220dp"
                android:layout_marginTop="7dp"
                android:background="@drawable/app_hosting_img"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="16dp" />

            <View
                android:id="@+id/view6"
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:layout_marginTop="7dp"
                android:background="@drawable/program_btn_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view5" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                 android:fontFamily="sans-serif"
                android:padding="10dp"
                android:layout_marginTop="10dp"
                android:text="Features 🌟"
                android:textColor="@color/tras_text_color"
                android:textSize="28dp"
                app:layout_constraintStart_toStartOf="@+id/view6"
                app:layout_constraintTop_toBottomOf="@+id/view5" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="15dp"
                android:fontFamily="@font/monospacebold"
                android:text="● Cheapest Price"
                android:textColor="@color/text_color"
                android:textSize="22dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView8" />

            <TextView
                android:paddingLeft="15dp"
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/monospacebold"
                android:text="● Free First Update"
                android:textColor="@color/text_color"
                android:textSize="22dp"
                app:layout_constraintStart_toStartOf="@+id/view6"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:paddingLeft="15dp"
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/monospacebold"
                android:text="● No Data Sharing"
                android:textColor="@color/text_color"
                android:textSize="22dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView25" />

            <View
                android:id="@+id/view8"
                android:layout_width="match_parent"
                android:layout_height="350dp"
                android:layout_marginTop="7dp"
                android:background="@drawable/program_btn_background"
                app:layout_constraintTop_toBottomOf="@+id/view6"
                tools:layout_editor_absoluteX="0dp" />

            <TextView
                android:id="@+id/textView27"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/monospacebold"
                android:padding="10dp"
                android:text="Pricing 🪙"
                android:textColor="@color/tras_text_color"
                android:textSize="28dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/view8" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:background="@drawable/program_btn_background"
                android:paddingLeft="7dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="10dp"
                android:paddingRight="7dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView27"
                app:srcCompat="@drawable/deploye" />

            <ImageView
                android:layout_marginLeft="50dp"
                android:layout_marginTop="10dp"
                android:id="@+id/imageView25"
                android:layout_width="120dp"
                android:layout_height="120dp"
                android:background="@drawable/program_btn_background"
                android:padding="10dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView6"
                app:srcCompat="@drawable/update" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="69dp"
                android:text="App Hosting"
                android:paddingLeft="25dp"
                android:textColor="@color/tras_text_color"
                android:textSize="22dp"
                app:layout_constraintStart_toEndOf="@+id/imageView6"
                app:layout_constraintTop_toTopOf="@+id/view8"
                android:fontFamily="@font/monospacebold"/>

            <TextView
                android:id="@+id/textView35"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="199dp"
                android:text="App Update's"
                android:paddingLeft="20dp"
                android:fontFamily="@font/monospacebold"
                android:textColor="@color/tras_text_color"
                android:textSize="22dp"
                app:layout_constraintStart_toEndOf="@+id/imageView25"
                app:layout_constraintTop_toTopOf="@+id/view8" />

            <TextView
                android:id="@+id/textView36"
                android:layout_width="190dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Host Your Application On Play Store At Just 150₹"
                android:textColor="@color/cardview_light_background"
                android:textSize="18dp"
                android:fontFamily="@font/monospacebold"
                app:layout_constraintStart_toEndOf="@+id/imageView6"
                app:layout_constraintTop_toBottomOf="@+id/textView34" />

            <TextView
                android:id="@+id/textView37"
                android:layout_width="190dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:fontFamily="@font/monospacebold"
                android:text="Update Your Application On Play Store At Just 50₹"
                android:textColor="@color/cardview_light_background"
                android:textSize="18dp"
                app:layout_constraintStart_toEndOf="@+id/imageView6"
                app:layout_constraintTop_toBottomOf="@+id/textView35" />

            <View
                android:id="@+id/view16"
                android:layout_width="match_parent"
                android:layout_height="850dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/program_btn_background"
                app:layout_constraintTop_toBottomOf="@+id/view8"
                tools:layout_editor_absoluteX="0dp" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fill The Form For Host Or Update Your App"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/monospacebold"
                android:textSize="15dp"
                android:textColor="@color/tras_text_color"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/view16" />

            <EditText
                android:fontFamily="@font/monospacebold"
                android:padding="3dp"
                android:id="@+id/ditTextName"
                android:layout_width="350dp"
                android:layout_height="40dp"
                android:layout_marginTop="100dp"
                android:background="@color/white"
                android:hint="Eg. Hosting Or Update"
                android:textColor="@color/application_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/view16" />

            <TextView
                android:id="@+id/textView40"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginBottom="10dp"
                android:text="Enter Your Requirement"
                android:textColor="@color/white"
                android:textSize="18dp"
                app:layout_constraintBottom_toTopOf="@+id/ditTextName"
                app:layout_constraintStart_toStartOf="@+id/view16" />

            <TextView
                android:id="@+id/textView41"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Enter Your Phone No."
                android:layout_marginLeft="24dp"
                android:layout_marginBottom="10dp"
                android:textColor="@color/white"
                android:textSize="18dp"
                app:layout_constraintBottom_toTopOf="@+id/editTextMobile"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/textView42"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="32dp"
                android:layout_marginTop="283dp"
                android:layout_marginLeft="24dp"
                android:layout_marginBottom="10dp"
                android:text="Enter Your E-mail ID"
                android:textColor="@color/white"
                android:textSize="18dp"
                app:layout_constraintStart_toStartOf="@+id/view16"
                app:layout_constraintTop_toTopOf="@+id/view16" />

            <EditText
                android:fontFamily="@font/monospacebold"
                android:padding="3dp"
                android:id="@+id/editTextMobile"
                android:layout_width="350dp"
                android:layout_height="40dp"
                android:layout_marginTop="210dp"
                android:background="@color/white"
                android:hint="+91 12345*****"
                android:textColor="@color/application_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/view16" />

            <EditText
                android:id="@+id/editTextEmail"
                android:layout_width="350dp"
                android:layout_height="40dp"
                android:background="@color/white"
                android:fontFamily="@font/monospacebold"
                android:hint="studydoc@gmail.com"
                android:textColor="@color/application_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView42" />

            <Button
                android:id="@+id/button3"
                android:layout_marginBottom="20dp"
                android:background="@drawable/button_style"
                android:text="Send Request"
                android:fontFamily="@font/monospacebold"
                android:textSize="22dp"
                android:layout_width="350dp"
                android:layout_height="55dp"
                android:layout_marginTop="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/editTextEmail" />

            <TextView

                android:id="@+id/textViewConfirmation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="505dp"
                android:text=""
                android:textColor="@color/white"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/view16" />



        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>