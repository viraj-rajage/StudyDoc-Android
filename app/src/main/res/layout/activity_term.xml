<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Term"
    android:background="@color/application_background">
    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_button" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:text="Project's"
        android:textColor="@color/text_color"
        android:fontFamily="monospace"
        android:textSize="24dp"
        app:layout_constraintBottom_toTopOf="@+id/view7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.355" />

    <View
        android:id="@+id/view7"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:background="@color/hover_color"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        tools:layout_editor_absoluteX="3dp"
        tools:ignore="MissingConstraints" />



    <View
        android:id="@+id/view14"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="4dp"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view17"
        android:layout_width="wrap_content"
        android:layout_height="400dp"
        android:background="@drawable/project_temp"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monospacebold"
        android:text="Why StudyDoc Project Market-"
        android:padding="20dp"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view17" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monospacebold"
        android:paddingLeft="25dp"
        android:text="-All Type Of Poject"
        android:textColor="@color/white"
        android:textSize="15dp"

        app:layout_constraintTop_toBottomOf="@+id/textView28"
        tools:layout_editor_absoluteX="58dp" />

    <TextView

        android:id="@+id/textView30"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monospacebold"
        android:paddingLeft="25dp"
        android:text="-Custom Requirements Based Project"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        tools:layout_editor_absoluteX="58dp" />

    <TextView

        android:id="@+id/textView31"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monospacebold"
        android:paddingLeft="25dp"
        android:text="-Price ₹50 To ₹1500"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintTop_toBottomOf="@+id/textView30"
        tools:layout_editor_absoluteX="56dp" />

    <View
        android:id="@+id/view18"
        android:layout_width="wrap_content"
        android:layout_height="170dp"
        android:background="@color/notes_button_color"
        app:layout_constraintBottom_toTopOf="@+id/view14"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/monospacebold"
        android:text="Project Type ✨"
        android:paddingLeft="30dp"
        android:paddingTop="10dp"
        android:textColor="@color/white"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view18" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:layout_marginTop="62dp"
        android:background="@drawable/program_btn_background"
        android:padding="10dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView29"
        app:layout_constraintStart_toStartOf="@+id/view18"
        app:layout_constraintTop_toTopOf="@+id/view18"
        app:srcCompat="@drawable/project_android" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="72dp"
        android:background="@drawable/program_btn_background"
        android:padding="10dp"
        android:layout_height="72dp"
        android:layout_marginTop="62dp"
        app:layout_constraintEnd_toStartOf="@+id/view12"
        app:layout_constraintTop_toTopOf="@+id/view18"
        app:srcCompat="@drawable/project_console" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:background="@drawable/program_btn_background"
        android:padding="10dp"
        android:layout_marginTop="62dp"
        app:layout_constraintStart_toEndOf="@+id/view12"
        app:layout_constraintTop_toTopOf="@+id/view18"
        app:srcCompat="@drawable/project_web" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:background="@drawable/program_btn_background"
        android:padding="10dp"
        android:layout_marginTop="62dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView30"
        app:layout_constraintTop_toTopOf="@+id/view18"
        app:srcCompat="@drawable/project_windows" />

    <View
        android:id="@+id/view12"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="83dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/view18"
        app:layout_constraintTop_toTopOf="@+id/view18" />

    <Button
        android:id="@+id/button"
        style="@style/button_design"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/white"
        android:fontFamily="@font/monospacebold"
        android:text="Get Project »"
        android:textColor="@color/application_background"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/view14"
        app:layout_constraintEnd_toEndOf="@+id/view14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view14" />
</androidx.constraintlayout.widget.ConstraintLayout>